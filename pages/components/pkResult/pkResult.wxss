/* pages/components/waitPk/waitPk.wxss */
.clearfix:after {
  visibility: hidden;
  display: block;
  font-size: 0;
  content: " ";
  clear: both;
  height: 0;
}
.pk-result{
  text-align: center;
}
.pk-result-mark{
  font-size: 79rpx;
  color: #8c79ed;
  display: block;
  float: left;
  padding: 60rpx;
}
.pk-result-ing{
  font-size: 26rpx;
  color: #a2a2a2;
  line-height: 40rpx;
}
.pk-result-vs{
  display: block;
  font-size: 40rpx;
  float: left;
  color: #6f6f6f;
  font-style: oblique;
}
.pk-result-text{
  font-size: 26rpx;
  color: #939393;
}